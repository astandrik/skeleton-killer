{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../dist/client",
    "rootDir": "./",
    "baseUrl": "./",
    "paths": {
      "phaser": ["../../node_modules/phaser/types/phaser.d.ts"]
    },
    "lib": ["DOM", "ES2020"],
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": [
    "**/*",
    "types/global.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}




